{
  "name": "Скрипты",
  "version": "1.0",
  "description": "Скрипты на заполнение",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "img/edit.png"
    },
    "icons": {
      "128": "img/edit.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://*/"
  ],
  "content_security_policy": "script-src 'self' https://pers21.github.io; object-src 'self'"
}
